# MongoDB Connection String
MONGO_URI=mongodb://localhost:27017/finquest

# Server Configuration
PORT=5000
NODE_ENV=development

# Session Secret (use a random string in production)
SESSION_SECRET=finquest_secret_key_change_this_in_production

# Google OAuth Credentials
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Important: Add these URLs to Google OAuth Authorized redirect URIs:
# - http://localhost:5000/auth/google/callback
# - http://localhost:3000
